{
    "name": "custom-server-typescript",
    "version": "1.0.0",
    "scripts": {
        "dev": "nodemon",
        "build": "next build && tsc --project tsconfig.server.json",
        "start": "cross-env NODE_ENV=production node dist/index.js"
    },
    "dependencies": {
        "@zeit/next-sass": "^1.0.1",
        "babel-plugin-module-resolver": "^3.2.0",
        "next": "latest",
        "next-routes": "^1.4.2",
        "node-sass": "^4.12.0",
        "react": "^16.8.4",
        "react-dom": "^16.8.4"
    },
    "devDependencies": {
        "@types/node": "^12.0.12",
        "@types/react": "^16.8.17",
        "@types/react-dom": "16.8.4",
        "cross-env": "^5.2.0",
        "nodemon": "^1.19.0",
        "ts-node": "^8.1.0",
        "typescript": "^3.4.5"
    },
    "moduleNameMapper": {
        "^.+\\.(css|sass|scss)$": "identity-obj-proxy",
        "@App/*": [
            "/*"
        ],
        "@Components/*": [
            "components/*"
        ]
    },
    "moduleFileExtensions": [
        "web.js",
        "js",
        "ts",
        "json",
        "web.jsx",
        "jsx",
        "tsx",
        "node"
    ]
}
